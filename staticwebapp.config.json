{
  "routes": [
    {
      "route": "/login*",
      "serve": "/.auth/login/aad",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "platformErrorOverrides": [
    {
      "statusCode": "401",
      "serve": "/.auth/login/aad"
    },
    {
      "errorType": "Unauthorized",
      "statusCode": "401",
      "serve": "/.auth/login/aad"
    }
  ],
  "defaultHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    "custom": "text/html"
  }
}